{
  "name": "multilingual-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:te": "cross-env DEFAULT_LOCALE=te SUPPORTED_LOCALES=te,en NODE_ENV=development next dev -p 3000",
    "dev:hi": "cross-env DEFAULT_LOCALE=hi SUPPORTED_LOCALES=hi PORT=3001 NODE_ENV=development next dev -p 3001",
    "dev:kn": "cross-env DEFAULT_LOCALE=kn SUPPORTED_LOCALES=kn PORT=3002 NODE_ENV=development next dev -p 3002",
    "dev:all": "concurrently \"npm run dev:te\" \"npm run dev:hi\" \"npm run dev:kn\"",
    "build": "next build",
    "build:te": "cross-env DEFAULT_LOCALE=te SUPPORTED_LOCALES=te,en next build",
    "build:hi": "cross-env DEFAULT_LOCALE=hi SUPPORTED_LOCALES=hi PORT=3001 next build",
    "build:kn": "cross-env DEFAULT_LOCALE=kn SUPPORTED_LOCALES=kn PORT=3002 next build",
    "build:all": "npm run build:te && npm run build:hi && npm run build:kn",
    "start": "next start -p 3000",
    "start:te": "cross-env DEFAULT_LOCALE=te SUPPORTED_LOCALES=te,en next start -p 3000",
    "start:hi": "cross-env DEFAULT_LOCALE=hi SUPPORTED_LOCALES=hi PORT=3001 next start -p 3001",
    "start:kn": "cross-env DEFAULT_LOCALE=kn SUPPORTED_LOCALES=kn PORT=3002 next start -p 3002",
    "start:all": "concurrently \"npm run start:te\" \"npm run start:hi\" \"npm run start:kn\"",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,scss}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,scss}\""
  },
  "dependencies": {
    "@apollo/client": "^4.0.0",
    "@auth/core": "^0.34.2",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@n8tb1t/use-scroll-position": "^2.0.3",
    "@types/nprogress": "^0.2.3",
    "add": "^2.0.6",
    "astronomy-bundle": "^7.7.7",
    "astronomy-engine": "^2.1.19",
    "autoprefixer": "^10.4.21",
    "bootstrap": "^5.3.8",
    "date-fns": "^4.1.0",
    "fs": "^0.0.1-security",
    "graphql": "^16.11.0",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "next": "^15.3.2",
    "next-auth": "^4.24.11",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "postcss-preset-env": "^10.1.6",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^19.1.0",
    "react-select": "^5.10.1",
    "react-slick": "^0.30.3",
    "reactjs-localstorage": "^1.0.1",
    "sass": "^1.89.0",
    "styled-components": "^6.1.18",
    "summernote": "^0.9.1",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/jquery": "^3.5.33",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/react-slick": "^0.23.13",
    "@types/reactjs-localstorage": "^1.0.2",
    "@types/summernote": "^0.8.10",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.27.0",
    "eslint-config-next": "15.4.0-canary.51",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "engines": {
    "node": "22.21.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      "Chrome >= 92",
      "Firefox >= 90",
      "Safari >= 15.4",
      "Edge >= 92",
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 Safari versions",
      "last 2 Edge versions",
      ">0.2%",
      "not dead",
      "not IE 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Firefox version",
      "last 1 Safari version"
    ]
  }
}
